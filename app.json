{
  "expo": {
    "name": "RND",
    "slug": "RND",
    "version": "0.2.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#04549C"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#04549C"
      },
      "package": "com.balu.mobile"
    },
    "web": {
      "favicon": "./assets/favicon.ico"
    }
  }
}
